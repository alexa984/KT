@model IS.Web.ViewModels.BranchViewModels.BranchViewModel   
@{
    ViewBag.Title = "Добавяне на клон";
}

<h2>@ViewBag.Title</h2>
@using (Html.BeginForm("AddBranch", "Branch", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>Добавяне на клон.</h4>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
  
            <div class="form-group">
                @Html.LabelFor(s => s.Town, new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.EnumDropDownListFor(s => s.Town, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(s => s.Address, new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.TextAreaFor(s => s.Address, new { @class = "form-control" })
                </div>
            </div>

        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Добавяне" />
        </div>








        }
